generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model health_check {
  id         Int       @id @default(autoincrement())
  updated_at DateTime? @default(now()) @db.Timestamptz(6)
}
